   "source": [
    "# We can also specify the format:  format=\"%Y-%d-%m %H:%M:%S\"\n",
    "program_df = pd.DataFrame({'date': ['25-07-2022', '26-07-2022', '27-07-2022','28-07-2022','29-07-2022'],\n",
    "                           'number_of_lectures': [2, 3, 2, 1, 1]})"
